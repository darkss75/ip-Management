# Production Environment Configuration
NODE_ENV=production
PORT=3000

# Database Configuration (현재는 파일 기반이므로 MongoDB 불필요)
# MONGODB_URI=mongodb://localhost:27017/ip_management

# Security - 반드시 변경하세요!
JWT_SECRET=your_company_super_secret_jwt_key_production_2024

# CORS - 회사 도메인으로 변경
FRONTEND_URL=https://your-company-domain.com

# Logging
LOG_LEVEL=warn

# Rate Limiting - 보안 강화
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=50